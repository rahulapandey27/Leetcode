bool carPooling(int** trips, int tripsSize, int* tripsColSize, int capacity) {
    int bus[1001];
    for ( int i=0; i<tripsSize; i++){
        int passenger= trips[i][0];
        int from=trips[i][1];
        int to=trips[i][2];
        bus[from]+=passenger;
        bus[to]-=passenger;
    }for(int i=1 ; i<1001; i++){
        bus[i]+=bus[i-1];
    }for(int i=0; i<1001; i++){
        if(bus[i]>capacity){
            return false;
        }
    }return true;
}
